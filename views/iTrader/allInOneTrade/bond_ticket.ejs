<div id="ticket-panel" class="panel panel-default panel-ticket msclear" style="height: 382px;">
    <div class="panel-heading">
        <span data-i18n="ticket_title"></span>
    </div>
    <div class="panel-body panel-first-row">
        <div class="row">
            <div class="bond-info col-lg-12 col-sm-12 col-sm-12">

            </div>
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="col-lg-12 col-md-12 col-sm-12 text-center">
                    <form class="form-horizontal form-trade">
                        <div class="form-group">
                            <label class="control-label col-sm-2" data-i18n="bond_symbol_code"></label>
                            <div class="col-sm-10 input-group input-group-sm">
                                <div class="input-group input-group-sm">
                                    <input type="text" name="symbol" id="ticket-input-symbol-code" class="form-control bond-input-symbol symbol-code" data-i18n="bond_symbol_code"
                                    required>
                                    <span class="input-group-addon">
                                        <a href="javascript:SearchProduct_Search('BOND','0');" tabindex="-1">
                                            <i class="fa fa-search text-muted"></i>
                                        </a>
                                    </span>
                                </div>
                            </div>
                        </div>
                        
                        <div class="form-group">
                            <label data-i18n="bond_PAR" class="col-sm-2 control-label"></label>
                            <div class="col-sm-10 input-group input-group-sm">
                                <input type="text" name="bondPAR" class="form-control bond-input-quantity" data-i18n="bond_PAR" required>
                            </div>
                        </div>
                    </form>
                </div>
                <div class="">
                    <button class="btn btn-success btn-sm col-sm-4 col-xs-4" data-typeSide='0' id="bond-buy-submit" type="submit" data-i18n="ticket_buy"
                        value="0" tabindex="6"></button>
                    <a class="col-sm-2 col-xs-2 col-sm-offset-1 col-xs-offset-1 btn btn-default btn-xs btn-clear ticket-btn-reset" tabindex="8"
                        data-i18n="ticket_form_reset"></a>
                    <button class="btn btn-danger btn-sm col-sm-4 col-xs-4  col-sm-offset-1 col-xs-offset-1" data-typeSide='1' id="bond-sell-submit"
                        type="submit" tabindex="7" value="1" data-container="body" data-toggle="popover" data-placement="right"
                        data-i18n="ticket_sell" data-content=""></button>
                </div>
            </div>

        </div>
