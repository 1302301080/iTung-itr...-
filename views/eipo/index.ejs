<div class="container-fluid" id="eipo-container">
    <div class="panel panel-default">
        <div class="panel-heading">
			<form class="form-inline">
            <span data-i18n="ipo-panel-title"></span>
            <div id="ipo-list-table-search-box-container" class="pull-right" style="margin-top:-5px;"></div>
				<div id="ipo-filter-group" class="pull-right" style="margin-right: 8px;">
				</div>
			</form>
        </div>
        <div class="panel-body">
            <div class="row">
                <div class="col-lg-12">
                    <div class="text-right login-account hide">
                        <span data-i18n="ipo-username-label"></span>
                        <span id="login-account"><%= user.id %></span>
                        <input type="hidden" value="<%= user.type %>" id="login-type" />
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    <table id="ipo-list-table" class="table table-bordered table-hover table-condensed table-striped">
                    </table>
                </div>
            </div>
        </div>
    </div>

    <div class="panel panel-default">
		<div class="panel-heading">
			<form class="form-inline">
				<span data-i18n="ipo-application-panel-title"></span>
				<div id="ipo-app-table-search-box-container" class="pull-right" style="margin-top:-5px;"></div>
				<div id="ipo-app-filter-group" class="pull-right" style="margin-right: 8px;"></div>
			</form>
			</div>
        <div class="panel-body">
            <div class="row">
                <div class="col-lg-12">
                    <table id="application-list-table" class="table table-bordered table-hover table-condensed table-striped">
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>