<div class="settlement_detail hidden" id="settlement_detail_SW_PWE">
        <div class="form-group form-group-sm">
            <label class="col-sm-3 control-label" data-i18n="settlement_SW_product_code"></label>
            <div class="col-sm-6">
                <input type="text" name="productCode" class="form-control productCode" required>
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label class="col-sm-3 control-label" data-i18n="settlement_SW_product_name"></label>
            <div class="col-sm-6">
                <input type="text" class="form-control pw-productName" readonly required>
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label class="col-sm-3 control-label" data-i18n="settlement_SW_holding"></label>
            <div class="col-sm-6">
                <input type="text" class="form-control pw-holding" name="holding" readonly required>
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label class="col-sm-3 control-label" data-i18n="settlement_SW_qty"></label>
            <div class="col-sm-6">
                <input type="text" name="amount" class="form-control input-amount" required>
            </div>
        </div>
        <div class="form-group form-group-sm">
            <label class="col-sm-3 control-label" data-i18n="settlement_SW_scrip"></label>
            <div class="col-sm-2">
                <input type="text" name="scrip" class="form-control">
            </div>
            <label class="col-sm-2 control-label" data-i18n="settlement_SW_quantity"></label>
            <div class="col-sm-2">
                <input type="text" name="x_quantity" class="form-control">
            </div>
            <span data-i18n="settlement_instruction_option"><span> 
        </div>
        <div class="form-group form-group-sm">
            <label class="col-sm-3 control-label" data-i18n="settlement_instruction_remark"></label>
            <div class="col-sm-6">
                <input type="text" name="remark" class="form-control">
            </div>
            <span data-i18n="settlement_instruction_option"><span> 
        </div>
    </div>